	var cvr_chart_data_json = $.ajax({
		url: "./i2b2Servlet/",
		dataType:"json",
		async: false,
		data: {step: "1",chart_type: "cvr"}
	}).responseText;

	var cvr_chart_data = new google.visualization.DataTable(cvr_chart_data_json);

