#dashboard_container{
	width: 98%;
	height: 98%;
}

#menu_container{
/*	border: solid 1px red; */
	float: left;
	width: 180px;
	background-color: #f7f8f7;
	margin-top: 10px;
	margin-right: 5px;
	border: 1px #bbbbbb solid;
}

#chart_container{
/*	border: solid 1px blue; */
	float: left;
}

span.menu_item_label{
   	font-family: verdana;
    font-size: 14px;
    font-weight: bold;
    padding-left: 10px;
}

div.menu_item{
	background-color: #f0f1f0;
	width: 100%;
	height: 25px;
	border-bottom: 1px #bbbbbb solid;
	color: #535b5b;
	padding-top: 5px;
}

div.menu_item_selected{
	background-color: #ffffff;
	border-right: 1px #ffffff solid;
	color: #000000;
}

div.menu_return{
	background-color: #f9bd16;;;
	color: #125e84;
	width: 100%;
	height: 25px;
	border-bottom: 1px #bbbbbb solid;
	padding-top: 5px;
	cursor: pointer;
}

.menu_section_item{
	background-color: #f9bd16;;;
	color: #125e84;
	width: 100%;
	height: 25px;
	border-bottom: 1px #bbbbbb solid;
	padding-top: 5px;
	margin-top: 50px;
	border-top: 1px #bbbbbb solid;
}

#step_item_1{
	background-color: #AECBE8;
}

#step_item_2{
	background-color: #7CA9D6;
}

#step_item_3{
	background-color: #6792BC;
}

div.step_item{
	width: 100%;
	height: 25px;
	border-bottom: 1px #bbbbbb solid;
	color: #ffffff;
	padding-top: 5px;
}

div.chart_item{
	float: left;
	background-color: #ffffff;
	border: solid 1px #bbbbbb;
	margin-left: 10px;
	margin-top: 10px;
/*	width: 600px; */
	width: 32%;
}

div.chart_title{
	background-color: #003265;
	font-family: verdana;
	font-weight: bold;
	color: #ffffff;
	padding: 3px;	
}

div.chart_subtitle{
	background-color: #e2e3e2;
	font-family: verdana;
	font-weight: bold;
	color: #000000;
	padding: 3px;
}

div.chart{
	width: 100%;
	height: 40%;
}


div.chart_all_ptns_btn{
	float: right; 
	background-color: lime;
}

div.step_two{
	width: 100%;
}

div.step_three{
	
}
div.chart_inner_btn{
/*	background-color: #F7C42A;
    border-top: 1px solid #BBBBBB; */
    color: #003265;
    cursor: pointer;
    font-family: verdana;
    font-weight: bold;
    padding: 3px;
}
div.chart_step_two{
	height: 90%;
}
.loader{
	position: absolute;
    z-index: 1000;
    top: 50px;
}
#menu_tooltip{
	background-color: #AECBE8;
	border-top: black;
	border-bottom: black;
	padding: 10px;
	margin-top: 0px;
	font-family: verdana;
}

#searchPatientBtn{
	background-color: #3787B2;
	width: 90%;
	margin-top: 5px;
	color: #ffffff;
	cursor: pointer;	
	height: 25px;
	font-family: verdana;
    font-size: 14px;
    font-weight: bold;
}

#singlePatientDataContainer{
	margin-top: 10px;
	margin-left: 10px;
}

#statusChartTitle{
	margin-left: 150px; 
	margin-bottom: 20px; 
	margin-top: 10px;
}
#scheduleTable{
	margin-left: 75px;
}
#statusChart{
	margin-left: 75px; margin-right: 140px;
}

.singlePatientMenuItemSelected{
	border: solid 1px #2a919c;
	float: left;
	cursor: pointer;
	background-color: #90cad2;
	margin-right: 1px;
	padding-top: 5px;
}

.singlePatientMenuItemLabelSelected{
	font-family: verdana;
    font-size: 14px;
    font-weight: bold;
    padding-left: 10px;
    color: #125e84;
}

.singlePatientMenuItem{
	border: solid 1px #125e84;
	float: left;
	cursor: pointer;
	background-color: #2a919c;
	margin-right: 1px;
	padding-top: 5px;
}

.singlePatientMenuItemLabel{
	font-family: verdana;
    font-size: 14px;
    font-weight: bold;
    padding-left: 10px;
    color: white;
}

.example-table {
	font-family: sans-serif;
	-webkit-font-smoothing: antialiased;
	font-size: 115%;
	width: auto;
	overflow: auto;
}
.example-table th {
	background-color: rgb(112, 196, 105);
	font-weight: normal;
	color: white;
	padding: 20px 30px;
	text-align: center;
}
.example-table td {
	background-color: rgb(238, 238, 238);
	padding: 20px 30px;
	color: rgb(111, 111, 111);
}
