/* basic positioning 
.legend li { float: left; margin-right: 10px; }*/
.legend {
	list-style: none;
	margin-top: 55px;
}

.legend span {
	border: 1px solid #ccc;
	float: left;
	width: 12px;
	height: 12px;
	margin: 2px;
}
/* your colors */
.legend .Irisk {
	background-color: #84d1ad;
}

.legend .IIrisk {
	background-color: #01ae76;
}

.legend .IIIrisk {
	background-color: #fdd687;
}

.legend .IVrisk {
	background-color: #ff944c;
}

.legend .Vrisk {
	background-color: #f34930;
}

.legend .VIrisk {
	background-color: #c479b1;
}
