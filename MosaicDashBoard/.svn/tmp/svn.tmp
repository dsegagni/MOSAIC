	processJSON = obj;
	
	var container = document.getElementById('path_chart');
	var processChart = new google.visualization.Timeline(container);

	var dataTable = new google.visualization.DataTable();
	dataTable.addColumn({ type: 'string', id: 'Story' });
	dataTable.addColumn({ type: 'string', id: 'Step' });
	dataTable.addColumn({ type: 'number', id: 'Start' });
	dataTable.addColumn({ type: 'number', id: 'End' });
	
	var dtRows = 0;
	for(k=0;k<obj.histories.length;k++){
		dtRows += obj.histories[k].steps.length;
	}
	
	dataTable.addRows(dtRows);
	
	var row = 0;

	for(i=0; i<obj.histories.length;i++){
		var start = 0;
		var path = obj.histories[i];
		for(j=0;j<path.steps.length;j++){
			dataTable.setCell(row, 0, path.label);
			dataTable.setCell(row, 1, path.steps[j].label);
			dataTable.setCell(row, 2, start*FROM_MS_TO_DAY);
			dataTable.setCell(row, 3, (start+parseInt(path.steps[j].time))*FROM_MS_TO_DAY);
			
			path.steps[j].fb=row;
			
			start+=parseInt(path.steps[j].time);
			row++;
		}
	}



	var options = {
			avoidOverlappingGridLines: false
	};

	processChart.draw(dataTable, options);
	//hide x-labels
	$($('svg', $('#path_chart')).children()[2]).hide();
	
	
	google.visualization.events.addListener(processChart, 'select', selectHandler);

	function selectHandler() {
		var selected = processChart.getSelection();
		//alert('A table row was selected');
		
		hideGraphs(2);
		showLoader();
		
		var p_num_param = "";
		var history ="";
		var step ="";
		
		for(i=0; i<processJSON.histories.length;i++){
			var steps = processJSON.histories[i].steps;
			for(j=0;j<steps.length;j++){
				if(steps[j].fb == selected[0].Fb){        //TO-DO: da controllare perchè non funziona
					p_num_param = steps[j].idcod;
					history = processJSON.histories[i].label;
					step = steps[j].label;
					break;
				}
			}
		}

		$.ajax({
			url: "./i2b2Servlet/",
			dataType:"json",
			async: true,
			data: { step: "3",
				chart_type: "comorb",
				patient_nums: p_num_param
			},
			complete: function(results){
				$('#step_item_2 > span').text("History: " + history);
				$('#step_item_3 > span').text("Step: " + step);
				$('#step_item_2').show();
				$('#step_item_3').show();
				showTooltip(3);
				hideLoader();
				setMenu(3);
				showGraphs(3);
				drawDrillDown(results.responseText);
			}
		});
	}
}

function drawDrillDown(dataJSON){
	//COMPLICATION CHART
	
	var drill_down_comorb_data = new google.visualization.DataTable(dataJSON);

	var drill_down_comorb_options = {
			chartArea:{top:20,width:"80%",height:"80%"},
			legend: {position: 'right', alignment: 'center'},
			pieSliceText: 'value'
	};

	var drill_down_comorb_chart = new google.visualization.PieChart(document.getElementById('drill_down_comorb_chart'));
	drill_down_comorb_chart.draw(drill_down_comorb_data, drill_down_comorb_options);
	//setting the process source of the drill down chart
	//$('#drill_down_comorb_chart').siblings('.chart_subtitle').text(processSource);

	//TIMETOCOMPLICATION
	
	var ttc_data = google.visualization.arrayToDataTable([
	                                                  ['Complication', '0-6', '6-12', '12-18','18-24','24-30','30-36'],
	                                                  ['Hypertension',  7, 8,9,10,4,2],
	                                                  ['Nephropathy',  5,9,2,4,7,11],
	                                                  ['Neuropathy',  2,7,1,0,8,19],
	                                                  ['Retinopathy',  4,12,9,7,4,1]
	                                                ]);

	var options = {
			isStacked: true,
	        hAxis: {title: 'Complications', titleTextStyle: {color: 'red'}},
	        vAxis: {title: 'Time in Months', titleTextStyle: {color: 'red'}},
	        legend: { position: 'top', maxLines: 3 },
	    	bar: { groupWidth: '75%' },
	    	series: {0:{color: '#F0F2B8'},
	            	 1:{color: '#D1E49F'},
	            	 2:{color: '#9BD087'},
	            	 3:{color: '#4CB972'},
	            	 4:{color: '#00A957'},
	            	 5:{color: '#008646'}
	            }
	       };

    var drill_down_timetocomp_chart = new google.visualization.ColumnChart(document.getElementById('drill_down_timetocomp_chart'));
    drill_down_timetocomp_chart.draw(ttc_data, options);
	
	
}

function backToProcess(){
	hideFilters(2);
	hideFilters(3);
	showTooltip(2);
	setMenu(2);
	hideGraphs(3);
	showGraphs(2);
}

function bindSearchPatient(){
	$('#searchPatientBtn').click(function(){
		var patient = $("#patientTxt").val();
		
		if(patient==null || patient==""){
			alert("Please inserta patient name");
		}
		else{
			hideFilters();
			hideLoader();
			hideGraphs(1);
			hideGraphs(2);
			hideGraphs(3);
			
			showSinglePatientData();
		}
	});
}

function showSinglePatientData(){
	
	$("#patientDetailsIntro").html("<b>Charts for Patient " + $("#patientTxt").val().toUpperCase() +"</b>");
		
	//show status timeline
	$("#singlePatientDataContainer").show();
	
	var patient_test = patients[0];
	var complex_rows = new Array();
	
	for(i=0; i<patient_test.complex_levels.length; i++){
		var start = patient_test.complex_levels[i][0].split("/");
		var end = patient_test.complex_levels[i][1].split("/");
		
		var row = new Array();
		
		row.push('Status');
		row.push(patient_test.complex_levels[i][2]);
		row.push(new Date(parseInt(start[2]), parseInt(start[1]), parseInt(start[0])));
		row.push(new Date(parseInt(end[2]), parseInt(end[1]), parseInt(end[0])));
		
		complex_rows.push(row);
	}
	
	var container = document.getElementById('statusChart');
	  var chart = new google.visualization.Timeline(container);
	  var dataTable = new google.visualization.DataTable();
	  dataTable.addColumn({ type: 'string', id: 'Status' });
	  dataTable.addColumn({ type: 'string', id: 'Level' });
	  dataTable.addColumn({ type: 'date', id: 'Start' });
	  dataTable.addColumn({ type: 'date', id: 'End' });
	  dataTable.addRows( complex_rows);

	  var options = {
	    timeline: { showRowLabels: true },
	    colors: ['#97D856', '#F2DF4D', '#FC8D32', '#E84619']
	  };

	  chart.draw(dataTable, options);
	  
	  //show drugs
	  var drugsChart = new google.visualization.ScatterChart(document.getElementById('drugsChart'));
	  
	  var drugs = new Array();
	  
	  for(i=0; i<patient_test.drugs.length; i++){
			var start = patient_test.drugs[i][0].split("/");
			var row = new Array();
			row.push(new Date(parseInt(start[2]), parseInt(start[1]), parseInt(start[0])));
			row.push(parseInt(patient_test.drugs[i][2]));
			
			drugs.push(row);
		}
	  
	  var drugsData = new google.visualization.DataTable();
	  drugsData.addColumn({ type: 'date', id: 'Date' });
	  drugsData.addColumn({ type: 'number', id: 'Measure' });
	  drugsData.setColumnLabel(0, 'date');
	  drugsData.setColumnLabel(1, 'Metformin');
	  
	  drugsData.addRows(drugs);
	  
      var drugsOption = {
              title: 'Drugs somministration'
            };
      
      drugsChart.draw(drugsData, drugsOption);
      
      
    //show cvr
	  var cvrChart = new google.visualization.LineChart(document.getElementById('cvrChart'));
	  
	  var cvr = new Array();
	  
	  for(i=0; i<patient_test.cvr.length; i++){
			var start = patient_test.cvr[i][0].split("/");
			var row = new Array();
			row.push(new Date(parseInt(start[2]), parseInt(start[1]), parseInt(start[0])));
			row.push(parseInt(patient_test.cvr[i][1]));
			
			cvr.push(row);
		}
	  
	  var cvrData = new google.visualization.DataTable();
	  cvrData.addColumn({ type: 'date', id: 'Date' });
	  cvrData.addColumn({ type: 'number', id: 'Measure' });
	  cvrData.addRows(cvr);
	  
      var cvrOption = {
              legend: 'none',
              curveType: 'function',
              title: 'Cardio Vascular Risk evolution',
              pointShape: 'cicle',
              pointSize: 6,
              animation:{
        	        duration: 1000,
        	        easing: 'out'
        	      }
            };
      
     cvrChart.draw(cvrData, cvrOption);
	
}
